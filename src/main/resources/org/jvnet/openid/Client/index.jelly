<j:jelly xmlns:j="jelly:core">
<html>
  <body>
    <j:if test="${it.openid!=null}">
      <div>
        Currently logged in as ${it.openid}
      </div>
    </j:if>
    <form action="start">
      OpenID: <input type="text" name="openid" size="100" />
      <br />
      <input type="submit" />
    </form>
  </body>
</html>
</j:jelly>
